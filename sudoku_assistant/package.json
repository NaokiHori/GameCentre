{
  "name": "sudoku_assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-prod": "vite build --base=https://naokihori.github.io/Trash/sudoku_assistant/",
    "check": "tsc",
    "dev": "vite",
    "prepare": "cd .. && husky sudoku_assistant/.husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier . --write"
    ]
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.7"
  }
}
